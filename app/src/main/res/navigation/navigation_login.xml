<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/navigation_login"
    app:startDestination="@id/loginMainFragment">

    <fragment
        android:id="@+id/loginMainFragment"
        android:name="moe.peanutmelonseedbigalmond.pixivfanboxviewer.ui.fragment.LoginMainFragment"
        android:label="LoginMainFragment">
        <action
            android:id="@+id/action_loginMainFragment_to_loginInputCookieFragment"
            app:destination="@id/loginInputCookieFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/loginInputCookieFragment"
        android:name="moe.peanutmelonseedbigalmond.pixivfanboxviewer.ui.fragment.LoginInputCookieFragment"
        android:label="LoginInputCookieFragment" />
</navigation>